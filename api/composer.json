{
  "name": "jeki-aka-zer0/hawaii",
  "type": "project",
  "license": "MIT",
  "description": "Pet project of Evgeniy Zhukov",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Evgeniy Zhukov",
      "email": "jekiakazer0@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "composer/package-versions-deprecated": "^1.0@dev",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "2.4.x-dev",
    "doctrine/doctrine-migrations-bundle": "3.1.x-dev",
    "doctrine/orm": "3.0.x-dev",
    "phpdocumentor/reflection-docblock": "5.x-dev",
    "sensio/framework-extra-bundle": "^6.1",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/intl": "*",
    "symfony/mailer": "*",
    "symfony/mime": "*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/notifier": "*",
    "symfony/process": "*",
    "symfony/property-access": "5.*",
    "symfony/property-info": "5.*",
    "symfony/proxy-manager-bridge": "5.*",
    "symfony/runtime": "*",
    "symfony/security-bundle": "*",
    "symfony/serializer": "5.*",
    "symfony/string": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "5.x-dev",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "5.x-dev",
    "symfony/css-selector": "5.x-dev",
    "symfony/debug-bundle": "5.x-dev",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "5.x-dev",
    "symfony/stopwatch": "5.x-dev",
    "symfony/var-dumper": "5.x-dev",
    "symfony/web-profiler-bundle": "5.x-dev"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Api\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.*"
    }
  }
}
