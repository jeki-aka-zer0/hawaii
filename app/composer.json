{
  "name": "jeki-aka-zer0/hawaii",
  "description": "Pet project of Evgeniy Zhukov",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "project",
  "license": "proprietary",
  "authors": [
    {
      "name": "Evgeniy Zhukov",
      "email": "jekiakazer0@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "*",
    "symfony/runtime": "*",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
